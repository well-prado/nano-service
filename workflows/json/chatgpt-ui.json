{
  "name": "ChatGPT UI",
  "description": "ChatGPT-like UI for interacting with OpenAI models",
  "version": "1.0.0",
  "trigger": {
    "http": {
      "method": "GET",
      "path": "/",
      "accept": "text/html"
    }
  },
  "steps": [
    {
      "name": "chatgpt",
      "node": "chatgpt-ui",
      "type": "module"
    }
  ],
  "nodes": {
    "chatgpt": {
      "inputs": {
        "title": "Nanoservice ChatGPT Interface",
        "model": "gpt-4o",
        "workflow_docs": true
      }
    }
  }
} 